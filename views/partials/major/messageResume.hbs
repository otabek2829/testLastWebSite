{{> header/admin-nav}}

<div class="CommandUser">
    <div class="title text-center my-5">
        <h2>{{title}} <span id="msgLength">{{sms}}</span> </h2>
    </div>
</div>
<section>
    <div class="container message-resume">
        <div class="row justify-content-between">
            {{#each data}}
            <div class="col-4 box my-2 ">
                <h5>имя : <span>{{this.name}}</span></h5>
                <h5>категория : <span>{{this.category}}</span></h5>
                <h5>телефон : <span>{{this.phoneNumber}}</span></h5>
                <h5>ссылка резюме : <span>{{this.adressResume}}</span></h5>
                <span>{{time}}</span>
                <div class="delete text-end">
                    <a href="/admin/messageResume/delete/{{this._id}}">
                        <i class="bi bi-trash3 text-danger "></i>
                    </a>
                </div>
            </div>
            {{/each}}
        </div>
    </div>
</section>

