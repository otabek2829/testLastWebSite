{{> header/admin-nav}}

<div class="CommandUser">
    <div class="title text-center my-5">
        <h2>{{title}}</h2>
    </div>
</div>

 <section>
     <!-- List Group Items -->
    <div class="about-list-group-items">
        <div class="container">
            <div class="row flex-column">
                <div class="col-12">
                    <div class="list-group" id="list-tab" role="tablist">

                        <!-- Button father -->

                        <!-- Разработка сайтов -->
                        
                         <a class="list-group-item " href="/admin/priceUslugi/webSite">Разработка сайтов</a>

                        <!-- SMM -->
                         <a class="list-group-item " href="/admin/priceUslugi/smm">SMM</a>

                        <!-- Логотипы, баннера -->
                         <a class="list-group-item " href="/admin/priceUslugi/logotipBanner">Логотипы, баннера</a>

                        <!-- Техническая поддержка -->
                         <a class="list-group-item " href="/admin/priceUslugi/technicalSupport">Техническая поддержка</a>
                        
                    </div>
                </div>

                <div class="col-12">
                    <div class="tab-content" id="nav-tabContent">

                        <!-- Button bosilganda u haqida ma\lumot chiqishi -->

                        <!-- Items title -->
                        <div class="items-title d-flex justify-content-between">
                            <div>
                                <h6>Название услуги</h6>
                            </div>
                            <div>
                                <h6>Стоимость услуги</h6>
                            </div>
                        </div>

                        <!-- Разработка сайтов -->
                        <div class="tab-pane fade show active" id="list-home" role="tabpanel"
                            aria-labelledby="list-home-list">
                            {{#each data}}
                            <div class="body-items-text">
                                <h6>{{this.uslugiTitle}}</h6>
                                <h6>от {{this.uslugiPrice}} сум</h6>
                            </div>
                            {{/each }}
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
 </section>

    {{!-- Bootstrap JS Ulanishi --}}
